package basics;

public class pyramid {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
  
		for(int i=3;i<=18;i++) {
			
			for(int j=3;j<=i;j++) {
				
			
	
				
				for(int k=j;k<=j;k++){
					System.out.print(k);
					System.out.print("\t");
			}
				
			System.out.println("");
		}
	}

}
}
